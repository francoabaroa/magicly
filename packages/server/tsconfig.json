{
  "compilerOptions": {
    "target": "es6",
    "lib": ["esnext"],
    "allowJs": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "baseUrl": ".",
    "paths": {
      "typings/*": [
        "src/typings/*"
      ],
      "models/*": [
        "src/models/*"
      ],
      "*": [
        "node_modules/*",
      ]
    }
  },
  "exclude": ["node_modules"],
  "include": ["**/*.ts"]
}